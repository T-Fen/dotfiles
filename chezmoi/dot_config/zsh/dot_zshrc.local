# Powerlevel10k
[[ -f ~/.p10k.zsh ]] && source ~/.p10k.zsh

# Prevent any stale zoxide alias from conflicting
unalias z 2>/dev/null

# Initialize zoxide (smart cd replacement)
eval "$(zoxide init zsh)"

# Show system info on terminal startup
fastfetch
